<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="195dp"
        android:layout_height="177dp"
        android:layout_marginEnd="8dp"
        android:onClick="oneclick"
        app:layout_constraintEnd_toStartOf="@+id/imageButton2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ablock" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="201dp"
        android:layout_height="177dp"
        android:onClick="onclick2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/a2" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="207dp"
        android:layout_height="19dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="4dp"
        android:paddingLeft="8dp"
        android:text="A Block"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="201dp"
        android:layout_height="18dp"
        android:layout_marginTop="4dp"
        android:paddingLeft="5dp"
        android:text="Agriculture Block"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton2" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="205dp"
        android:layout_height="129dp"
        android:layout_marginTop="4dp"
        android:onClick="onclick3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/cblock" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="195dp"
        android:layout_height="128dp"
        android:layout_marginTop="4dp"
        android:onClick="onclick4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton3"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/d2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="205dp"
        android:layout_height="18dp"
        android:paddingLeft="5dp"
        android:text="C Block"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="198dp"
        android:layout_height="19dp"
        android:paddingLeft="5dp"
        android:text="D and E Block"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/imageButton5" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="199dp"
        android:layout_height="156dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:onClick="onclick5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/gandhblock" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="194dp"
        android:layout_height="155dp"
        android:layout_marginTop="4dp"
        android:onClick="onclick6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.384"
        app:layout_constraintStart_toEndOf="@+id/imageButton6"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:srcCompat="@drawable/lib" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="205dp"
        android:layout_height="19dp"
        android:paddingLeft="5dp"
        android:text="G and H Block"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="198dp"
        android:layout_height="20dp"
        android:layout_marginTop="4dp"
        android:paddingLeft="5dp"
        android:text="Library"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/imageButton7" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="202dp"
        android:layout_height="133dp"
        android:onClick="onclick7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:srcCompat="@drawable/ai" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="193dp"
        android:layout_height="130dp"
        android:layout_marginStart="8dp"
        android:onClick="onclick8"
        app:layout_constraintStart_toEndOf="@+id/imageButton9"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:srcCompat="@drawable/oat" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="202dp"
        android:layout_height="20dp"
        android:paddingLeft="5dp"
        android:text="AI Block"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton9" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="195dp"
        android:layout_height="21dp"
        android:paddingLeft="5dp"
        android:text="OAT"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/imageButton11" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="379dp"
        android:layout_height="34dp"
        android:paddingLeft="90dp"
        android:text="Select Where Are You Now..."
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.997" />

</androidx.constraintlayout.widget.ConstraintLayout>